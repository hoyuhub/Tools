
@{
    ViewData["Title"] = "播放";
}
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/videojs/video.min.js"></script>
<script src="~/videojs/lang/zh-CN.js"></script>
<link href="~/videojs/video-js.min.css" rel="stylesheet" />
<script>
    $(function () {
        var player = videojs('my-video', {
            controls: true,
            autoplay: false,
            preload: 'auto',
            fluid: true,
            aspectRatio: '16:9',
            playbackRates: [0.5, 1, 1.5, 2],
            poster:'@Html.Raw(@ViewData["imgPath"])'
        });
         player.src('@Html.Raw(@ViewData["playPath"])')
    })

</script>
<div class="row">
    <video id="my-video" class="video-js vjs-fluid vjs-default-skin vjs-big-play-centered">
    </video>

</div>
